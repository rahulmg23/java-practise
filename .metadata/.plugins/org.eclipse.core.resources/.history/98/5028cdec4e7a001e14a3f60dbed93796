package com.kn.patterns;

import java.util.Iterator;

public class Pattern3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k =0;
		for (int i = 1; i <=9; i++) {
			if(i<=5) {
				k++;
			}
			else {
				k--;
			}
		for (int j = 1; j <=5; j++) {
			if(j>=1 && j <=k) {
				System.out.print("*");
			}
			else {
				System.out.println(" ");
			}
			System.out.println( );
		}
		}
		}

	}


