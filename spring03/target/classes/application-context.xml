<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="trainer1" class="com.kn.entities.Trainer">

		<property name="id" value="1234"></property>
		<property name="name" value="rahul"></property>
		<property name="salary" value="1234"></property>
	</bean>

	<bean id="university" class="com.kn.entities.University">

		<property name="id" value="123422"></property>
		<property name="name" value="rahulvtu"></property>
		<property name="trainer" ref="trainer1"></property>

	</bean>



</beans>